import app.suprsend.android.event.EventModel;

CREATE TABLE UserEventTable (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    model   TEXT as EventModel,
    isDirty INTEGER NOT NULL
);

track:
INSERT OR FAIL INTO UserEventTable(id,model,isDirty) VALUES (?,?,?);


getTrackedEvents:
SELECT *
FROM UserEventTable WHERE isDirty =? LIMIT :limit;

delete:
DELETE FROM UserEventTable WHERE id IN ?;